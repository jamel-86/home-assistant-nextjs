{
  "name": "Next.js Add-on",
  "version": "1.0",
  "slug": "nextjs_addon",
  "description": "A Next.js add-on for Home Assistant",
  "arch": ["armv7", "armhf", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:3000",
  "host_network": true,
  "ports": {
    "3000/tcp": 3000
  },
  "map": [
    "config:rw"
  ],
  "options": {},
  "schema": {
    "port": "int",
    "host": "string"
  }
}
